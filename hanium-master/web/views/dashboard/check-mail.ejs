                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- end pageheader  -->
                    <!-- ============================================================== -->
                </div>
            </div>
            <div class="col-xl-12 col-lg-12 col-md-6 col-sm-12 col-12">
	            <form action="write" method="post">
			        <div class="card">
			            <div class="card-header">
			                <h5 class="mb-0">Mail List</h5>
			            </div>
			            <div class="card-body">
			                <div class="table-responsive">
	                            <table class="table">
	                                <thead class="bg-light">
	                                    <tr class="border-0">
	                                        <th class="border-0">#</th>
	                                        <th class="border-0">User Email</th>
	                                        <th class="border-0">Subject</th>
	                                        <th class="border-0">Content</th>
	                                        <th class="border-0">Date</th>
	                                    </tr>
	                                </thead>
	                                <tbody>
	                                	<% for(var i=0; i<data.length; i++){ %>
	                                    <tr>
	                                        <td><%= i %></td>
	                                        <td><%= data[i].from %></td>
	                                        <td><%= data[i].subject %></td>
	                                        <td><%= data[i].content %></td>
	                                        <td><%= data[i].date %></td>
	                                    </tr>                                                    
	                                    <% } %>
	                                    <tr>
	                                        <td colspan="5">
	                                        	<a href="write" class="btn btn-outline-light float-right">Write</a>
	                                        	<a href="reset" class="btn btn-outline-light float-right">Reset</a>
	                                        </td>
	                                    </tr>
	                                </tbody>
	                            </table>
	                        </div>
			            </div>
			        </div>
			    </form>
            </div>
